{
  "N8NSettings": {
    "BaseUrl": "https://howardmei.app.n8n.cloud",
    "OrdersApiUrl": "https://howardmei.app.n8n.cloud/webhook/get-order",
    "SubscribersApiUrl": "https://howardmei.app.n8n.cloud/webhook/get-subscribe",
    "ApiKey": "",
    "Timeout": 30,
    "WebhookSecret": "",
    "HealthCheckIntervalMinutes": 60,
    "HealthCheckEnabled": true,
    "WebhookEndpoints": {
      "OrderUpdated": "/api/n8nwebhook/order-updated",
      "DataChanged": "/api/n8nwebhook/data-changed",
      "Health": "/api/n8nwebhook/health"
    }
  },
  "PaymentSettings": {
    "SupportedMethods": [
      "CreditCard",
      "BankTransfer"
    ],
    "DefaultCurrency": "TWD"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApplicationSettings": {
    "PageSize": 20,
    "MaxPageSize": 100,
    "DateFormat": "yyyy-MM-dd",
    "TimeZone": "Asia/Taipei"
  }
}